{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier",
        "prettier/prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "no-unused-vars": "warn",
        "no-useless-escape": "off",
        "no-console": "off",
        "no-use-before-define": "off",
        "import/extensions": "off",
        "prefer-regex-literals": "off"
    }
}
